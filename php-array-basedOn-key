// combine two array and created new array
$a = array('a', 121212');
$b = array('b', 232323);
$c = array_combine($a, $b);
$newarr = array();
$key1 = array_keys($c);
foreach($c as $key => $value){
$newarr[] = $key ." ".$value;
}
echo "<pre>";
print_r($newarr);
